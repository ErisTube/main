{
	"name": "eristube",
	"description": "A powerful music player for your Discord bot on Eris.",
	"version": "1.0.0-dev.1737959292",
	"main": "index.js",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"build:del": "rimraf dist && tsc -p tsconfig.json",
		"build:docs": "docgen -s src/*.ts src/**/*.ts -o docs/index.json -c docs/index.yml -g -S 1 -j .jsconfig.json",
		"pub": "npm publish",
		"pub:dev": "npm publish --tag dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ErisTube/main.git"
	},
	"keywords": [
		"bot",
		"eris",
		"music",
		"discord",
		"eris bot",
		"discord bot",
		"discord music",
		"discord player",
