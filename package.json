		"discord player music",
		"eristube"
	],
	"author": "ErisTube Inc.",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/ErisTube/main/issues"
	},
	"dependencies": {
		"@distube/ytdl-core": "^4.15.9",
		"axios": "^1.7.9",
		"eris": "github:abalabahaha/eris#dev"
	},
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@babel/plugin-transform-class-properties": "^7.25.9",
		"@babel/plugin-transform-object-rest-spread": "^7.25.9",
		"@babel/preset-env": "^7.26.0",
		"@babel/preset-typescript": "^7.26.0",
		"@distube/docgen": "github:distubejs/docgen",
		"@types/node": "^22.10.5",
		"@types/yt-search": "^2.10.3",
		"jsdoc-babel": "^0.5.0",
		"rimraf": "^6.0.1",
